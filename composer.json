{
    "name": "linkorb/userbase-server",
    "description": "UserBase: Identity + Login + Signup Service",
    "homepage": "http://www.github.com/linkorb/userbase-server",
    "keywords": ["login", "signup", "user", "account"],
    "type": "library",
    "authors": [{
        "name": "Joost Faassen",
        "email": "j.faassen@linkorb.com",
        "role": "Development"
    }],
    "require": {
        "php": ">=5.3.0",
        "symfony/console": "~2.7",
        "symfony/config": "~2.7",
        "symfony/yaml": "~2.7",
        "symfony/form": "~2.7",
        "symfony/security": "~2.7",
        "silex/silex": "2.0.x-dev#a3881a5a2fc7e0233307c8240d4adc909a559b49",
        "crodas/service-provider": "~0.1",
        "twig/twig": ">=1.8,<2.0-dev",
        "symfony/twig-bridge": "~2.7",
        "guzzlehttp/guzzle": "~4.2",
        "monolog/monolog": "~1.6",
        "swiftmailer/swiftmailer": "~5.3",
        "html2text/html2text": "~2.0",
        "herald-project/client-php": "~1.0",
        "symfony/translation": "~2.7",
        "firebase/php-jwt": "~2.0",
        "bshaffer/oauth2-server-php": "^1.7",
        "league/oauth2-client": "^0.11.0",
        "bshaffer/oauth2-server-httpfoundation-bridge": "^1.0",
        "symfony/validator": "~2.7"
    },
    "require-dev": {
        "doctrine/dbal": "~2.5",
        "dbtk/schema-loader": "~2.1",
        "linkorb/haigha": "~2.2",
        "linkorb/conventions-checker": "~1.1"
    },
    "extra": {
        "grumphp": {
            "config-default-path": "vendor/linkorb/conventions-checker/config/app/grumphp.yml"
        }
    },
    "autoload": {
        "psr-4": {
            "UserBase\\Server\\": "src/"
        }
    }
}
